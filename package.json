{
  "name": "frontend-webpack-starter",
  "version": "1.0.1",
  "description": "Frontend webpack starter for building static websites",
  "main": "index.js",
  "author": "Anton Zdanov <akumael@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.9",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.16",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.0.10",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "scripts": {
    "build:watch:prod": "yarn watch --env.env=prod",
    "build:watch:dev": "yarn watch --env.env=dev",
    "build:watch": "yarn build --watch",
    "build:dev:analyze": "yarn build --env.env=dev --env.addons=bundleanalyze",
    "build:dev": "yarn build --env.env=dev",
    "debug:dev": "yarn debug --env.env=dev",
    "build:prod:analyze": "yarn build --env.env=prod --env.addons=bundleanalyze",
    "build:prod": "yarn build --env.env=prod",
    "debug:prod": "yarn debug --env.env=prod",
    "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "build": "webpack"
  }
}
